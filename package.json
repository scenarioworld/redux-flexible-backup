    "type": "git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "redux-flexible-backup",
  "author": "Brook Warner Jensen",
  "description": "Flexible Redux state backup/restore and undo/redo systems",
  "module": "dist/redux-flexible-backup.esm.js",
  "size-limit": [
    {
      "path": "dist/redux-flexible-backup.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/redux-flexible-backup.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@size-limit/preset-small-lib": "^4.9.0",
    "@types/lodash": "^4.14.167",
    "husky": "^4.3.0",
    "object-sizeof": "^1.6.1",
    "size-limit": "^4.9.0",
    "ts-jest": "^26.4.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "jsondiffpatch": "^0.4.1",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.20"
  }
